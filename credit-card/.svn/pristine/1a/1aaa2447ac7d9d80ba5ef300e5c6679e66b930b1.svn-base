<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pay.aile.bill.mapper.CreditAccountTypeMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.pay.aile.bill.entity.CreditUserFileRelation">
		<id column="id" property="id" />
		<result column="user_id" property="userId" />
		<result column="file_id" property="fileId" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_id as userId, file_id as fileId
    </sql>
     <insert id="batchInsert" parameterType="java.util.List">
        insert credit_user_file_relation (user_id,file_id)
        values
        <foreach collection="list" item="item" index="index" separator="," >  
        (#{item.userId},#{item.fileId})          
         </foreach>
    </insert>
</mapper>
